{
  "env": {
    "browser": true,
    "jquery": true,
    "node": true,
    "phantomjs": true
  },
  "globals": {
    "require": true,
    "define": true,
    "describe": true,
    "after": true,
    "afterEach": true,
    "before": true,
    "beforeEach": true,
    "expect": true,
    "it": true,
    "inject": true,
    "spyOn": true
  },
  "parserOptions": {
    "ecmaVersion": 6
  },
  "rules": {
    "strict": 0,
    "camelcase": 2,
    "curly": [
      2,
      "all"
    ],
    "eqeqeq": 2,
    "wrap-iife": [
      2,
      "any"
    ],
    "indent": [
      2,
      2,
      {
        "SwitchCase": 1
      }
    ],
    "new-cap": 2,
    "no-caller": 2,
    "quotes": [
      2,
      "single"
    ],
    "no-undef": 2,
    "no-unused-vars": 2,
    "semi": [
      2,
      "always"
    ],
    "no-cond-assign": 0,
    "no-debugger": 0,
    "comma-style": [
      2,
      "last"
    ],
    "complexity": [
      2,
      5
    ],
    "no-empty": 2,
    "no-with": 2,
    "brace-style": 2,
    "no-mixed-spaces-and-tabs": 2,
    "no-multi-spaces": 2,
    "operator-linebreak": [
      2,
      "after"
    ],
    "key-spacing": [
      2,
      {
        "beforeColon": false,
        "afterColon": true
      }
    ],
    "space-unary-ops": 0,
    "no-spaced-func": 2,
    "space-before-function-paren": [
      2,
      "never"
    ],
    "space-in-parens": [
      2,
      "never"
    ],
    "comma-dangle": [
      2,
      "never"
    ],
    "no-trailing-spaces": 2,
    "yoda": [
      2,
      "never"
    ],
    "dot-notation": [
      2,
      {
        "allowPattern": "^[a-z]+(_[a-z]+)+$"
      }
    ],
    "newline-after-var": [
      2,
      "always"
    ],
    "eol-last": 2,
    "one-var": [
      2,
      "never"
    ],
    "space-return-throw-case": 0,
    "keyword-spacing": [
      2
    ],
    "lines-around-comment": [
      2,
      {
        "afterBlockComment": false,
        "afterLineComment": false
      }
    ],
    "space-infix-ops": 2,
    "space-before-blocks": [
      2,
      "always"
    ],
    "consistent-this": [
      2,
      "self"
    ],
    "linebreak-style": [
      2,
      "unix"
    ]
  }
}
